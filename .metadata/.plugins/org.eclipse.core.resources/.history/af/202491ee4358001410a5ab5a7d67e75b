
public class Class {
	
	private Discipline discipline;
	private ClassTime time;
	
	public Class (Discipline disc, int weekDay, int hour, int minute){
		this.discipline = disc;
		time = new ClassTime (weekDay, hour, minute);
	}
	
	public ClassTime getClassTime(){
		return time;
	}
	
	public Discipline getDiscipline(){
		return discipline;
	}
}
