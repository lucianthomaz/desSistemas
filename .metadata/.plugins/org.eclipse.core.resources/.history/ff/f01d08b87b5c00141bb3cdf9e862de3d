DELETE TABLE GEFDATABASE.COURSE;

CREATE TABLE GEFDATABASE.Course (CODE_COURSE VARCHAR(5) NOT NULL,
CREDIT INTEGER NOT NULL,
NAME VARCHAR(20) NOT NULL,
MODULE INTEGER NOT NULL,
PRIMARY KEY (CODE_COURSE));

CREATE TABLE GEFDATABASE.StudentsClass ( CODE_STUDENTS_CLASS INTEGER NOT NULL, 
CODE_COURSE VARCHAR(5) NOT NULL,
PRIMARY KEY(CODE_STUDENTS_CLASS, CODE_COURSE),
FOREIGN KEY(CODE_COURSE) REFERENCES Course("CODE_COURSE"));

CREATE TABLE GEFDATABASE.ClassTime( CODE_STUDENTS_CLASS INTEGER NOT NULL,
ID_COURSE INTEGER NOT NULL,
DAY_OF_WEEK INTEGER NOT NULL,
CLASS_TIME VARCHAR(1) NOT NULL);